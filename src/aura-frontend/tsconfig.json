{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "postcss": ["node_modules/postcss"]
    },
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noEmit": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": ".tsbuild",
    "declarationDir": ".tsbuild/types",
    "allowJs": true,
    "skipLibCheck": true,
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src",
    "vite.config.ts",
    "vite.config.js"
  ],
  "exclude": [
    "node_modules"
  ]
}


